        "ServerScripts": [
          {
            "Title": "社員番号・氏名の自動入力",
            "Name": "",
            "BeforeOpeningPage": true,
            "Body": "try{\n    // 新規作成の時のみ実行\n    if ( context.Action === 'new' ) {\n        // 通常、新規作成時は全項目がサーバに送信されるが、読取専用項目は対象外\n        // スクリプトで初期値をセットした読取専用項目も登録するにはこの記述が必要\n        // 参考マニュアル：https://pleasanter.org/ja/manual/faq-set-value\n        columns.ClassA.ExtendedControlCss=\"always-send\";        // 社員番号\n        columns.ClassB.ExtendedControlCss=\"always-send\";         // 氏名\n        model.ClassA = context.UserId; //社員番号\n        model.ClassB = context.UserId; //氏名\n    }\n}catch(e){\n    context.Log(e.stack);\n}",
            "Id": 1
          },
          {
            "Title": "本人確認期限の10日前設定",
            "Name": "",
            "BeforeUpdate": true,
            "Body": "// 内容：本人確認期限を更新したタイミングで本人確認期限_周知の日付をその10日前に設定する\n// 補足：条件は更新前\ntry {\n    // 旧値と新値を取得\n    const oldDateObj = saved.DateA ? saved.DateA : null;\n    const newDateObj = model.DateA ? model.DateA : null;\n//    context.Log('saved[\"本人確認期限\"]=' + saved.DateA + ' / model[\"本人確認期限\"]=' + model.DateA);\n\n    if (oldDateObj !== newDateObj ) {\n        if (newDateObj) {\n            const notice = new Date(newDateObj);\n            notice.setDate(notice.getDate() - 10);  // 周知日を10日前に設定\n            model.DateC = notice;\n        } else {\n            // 本人確認期限をクリアした時は周知日もクリア\n            model.DateC = null;\n        }\n    }\n} catch (e) {\n    context.Log(e.stack);\n}",
            "Id": 3
          },
          {
            "Title": "電話番号の入力チェック",
            "Name": "",
            "BeforeCreate": true,
            "BeforeUpdate": true,
            "Body": "try {\n    var phone1 = model.ClassF; // 固定電話の入力\n    var phone2 = model.ClassG; // 携帯電話の入力\n    var phone3 = model.ClassH; // 公用携帯の入力\n\n\n    var phone4 = model.ClassP; // 電話番号(固定)の入力\n    var phone5 = model.ClassQ; // 電話番号(携帯)の入力\n\n\n    var selections = model.ClassI || []; // 複数選択プルダウン（分類I）\n\n    // 1. 入力がすべて空ならエラー\n    if (!phone1 && !phone2 && !phone3) {\n        context.Error('固定電話、携帯電話、公用携帯のいずれかを入力してください。');\n    }\n　 if (!phone4 && !phone5) {\n        context.Error('電話番号(固定)、電話番号(携帯)のいずれかを入力してください。');\n    }\n\n//     2. 入力があるのに、対応する分類が選ばれていない場合\n//    if (phone1 && !selections.includes('固定電話')) {\n//       context.Error('固定電話が入力されていますが、プルダウンで「固定電話」が選択されていません。');\n//   }\n//    if (phone2 && !selections.includes('携帯電話')) {\n//       context.Error('携帯電話が入力されていますが、プルダウンで「携帯電話」が選択されていません。');\n//   }\n//  if (phone3 && !selections.includes('公用携帯')) {\n//      context.Error('公用携帯が入力されていますが、プルダウンで「公用携帯」が選択されていません。');\n//   }\n\n    // 3. 分類で選択されているのに、対応する入力がない場合\n    if (selections.includes('固定電話') && !phone1) {\n        context.Error('「固定電話」がプルダウンで選択されていますが、入力がありません。');\n    }\n    if (selections.includes('携帯電話') && !phone2) {\n        context.Error('「携帯電話」がプルダウンで選択されていますが、入力がありません。');\n    }\n    if (selections.includes('公用携帯') && !phone3) {\n        context.Error('「公用携帯」がプルダウンで選択されていますが、入力がありません。');\n    }\n\n} catch (e) {\n    context.Log(e.stack);\n}",
            "Id": 4
          }
        ],
